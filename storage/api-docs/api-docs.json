                "operationId": "a6b80d711f984d4ba80bbeef0dba488b",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Domain ID",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Domain details with whois",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/DomainResource"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Domain not found"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            },
            "put": {
                "tags": [
                    "Domains"
                ],
                "summary": "Update domain WHOIS info",
