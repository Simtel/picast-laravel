                "operationId": "d14ff0419c757eeeba3b8b6b7bb202cc",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Domain ID",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "WHOIS info updated successfully"
                    },
                    "404": {
                        "description": "Domain not found"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            },
            "delete": {
                "tags": [
                    "Domains"
                ],
                "summary": "Delete domain",
                "description": "Delete domain\n\nDelete domain and associated WHOIS history",
                "operationId": "8b38cc69f63d6d243271870316f67a39",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Domain ID",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Domain deleted successfully"
                    },
                    "404": {
                        "description": "Domain not found"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
