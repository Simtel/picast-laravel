    "components": {
        "schemas": {
            "DomainResource": {
                "description": "Domain resource with WHOIS info",
                "properties": {
                    "id": {
                        "description": "Domain ID",
                        "type": "integer"
                    },
                    "name": {
                        "description": "Domain name",
                        "type": "string",
                        "example": "example.com"
                    },
                    "expire_at": {
                        "description": "Expiration date",
                        "type": "string",
                        "format": "date-time"
                    },
                    "whois": {
                        "description": "WHOIS information",
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                },
                "type": "object"
            },
            "VideoFullResource": {
                "title": "Video Full Resource",
                "description": "Полный ресурс видео",
                "required": [
                    "id",
                    "url"
                ],
                "properties": {
                    "id": {
                        "description": "ID видео",
                        "type": "integer"
                    },
                    "title": {
                        "description": "Заголовок видео",
                        "type": "string"
                    },
                    "url": {
                        "description": "URL видео",
                        "type": "string"
                    },
                    "createdAt": {
                        "description": "Дата создания",
                        "type": "string",
                        "format": "date-time"
                    },
                    "updatedAt": {
                        "description": "Дата обновления",
                        "type": "string",
                        "format": "date-time"
                    }
                },
                "type": "object"
            },
            "VideoResource": {
                "title": "Video Resource",
                "description": "Ресурс видео",
                "required": [
                    "id",
                    "url"
                ],
                "properties": {
                    "id": {
                        "description": "ID видео",
                        "type": "integer"
                    },
                    "url": {
                        "description": "URL видео",
                        "type": "string"
                    }
                },
                "type": "object"
            }
        },
        "securitySchemes": {
            "sanctum": {
                "type": "apiKey",
                "description": "Bearer token для аутентификации",
                "name": "Authorization",
                "in": "header"
            }
        }
