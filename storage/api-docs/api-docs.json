    "paths": {
        "/api/v1/domains": {
            "get": {
                "tags": [
                    "Domains"
                ],
                "summary": "Get all user domains",
                "description": "Show all user domains\n\nShow all user domains without whois history",
                "operationId": "16df26494f7664a293dc215ab81741ce",
                "responses": {
                    "200": {
                        "description": "List of user domains",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/DomainResource"
                                    }
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            },
            "post": {
                "tags": [
                    "Domains"
                ],
                "summary": "Create new domain",
                "description": "Store new domain\n\nStore new domain for authenticated user",
                "operationId": "00a8b9af9754e614f32f5cc29a4944b6",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name"
                                ],
                                "properties": {
                                    "name": {
                                        "description": "Domain name",
                                        "type": "string",
                                        "example": "example.com"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Domain created successfully"
                    },
                    "422": {
                        "description": "Validation error"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
